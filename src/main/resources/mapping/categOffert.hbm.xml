<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jst.objects.categories.CategOffert" table="categorieoffert">
        <id name="id" type="integer" column="CategorieOffertID">
            <generator class="increment" />
        </id>
        <property name="label" type="string" column="CategorieOffertLibelle" />
    </class>
</hibernate-mapping>
