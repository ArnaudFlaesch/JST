<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jst.objects.categories.Role" table="role">
        <id column="RoleID" name="id" type="integer">
            <generator class="increment"/>
        </id>
        <property column="RoleLibelle" name="label" type="string"/>
    </class>
</hibernate-mapping>
